{
  "expo": {
    "name": "E-novation",
    "slug": "E-novation",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/iconcl.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashclient.png",
      "resizeMode": "cover",
      "backgroundColor": "#f2ca30"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "bundlename",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan barcodes on event tickets."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/iconcl.png",
        "backgroundColor": "#f2ca30"
      },
      "package": "com.arowanax.Enovation"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "oneSignalAppId": "c6c6bb5f-bb26-495a-9d24-6a1322b5d270",
      "eas": {
        "projectId": "b3aa5e7b-c9d3-453c-bb12-018107aeea3b"
      }
    },
    "plugins": [
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ]
  }
}
